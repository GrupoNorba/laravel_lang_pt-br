{
    "name": "gruponorba/laravel_lang_pt-br",
    "type": "library",
    "description": "Laravel validation messages in Portuguese",
    "homepage": "https://github.com/GrupoNorba/laravel_lang_pt-br",
    "license": "MIT",
    "authors": [
        {
            "name": "norbberto",
            "email": "norbberto.gn@gmail.com",
            "homepage": "https://github.com/GrupoNorba",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "composer-runtime-api": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "GrupoNorba\\PtBrLang\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrupoNorba\\PtBrLang\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel" : {
            "providers": [
                "GrupoNorba\\PtBrLang\\Providers\\PtBrLangServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
